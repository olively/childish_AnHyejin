<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
  
  <mapper namespace="com.bamboo.mapper.TestMapper">
  
  
  <select id="listPage" resultType="Test_DTO">
  <![CDATA[
  	select * from board
  	where bid > 0
  	order by bid desc, regdate desc
  	limit #{page}, 10
  ]]>
  </select>
  </mapper>